            <descriptiveNote>
                <p>Not repeatable.</p>
            </descriptiveNote>
        </localTypeDeclaration>

        <localTypeDeclaration>
            <abbreviation>place</abbreviation>
            <citation>Places associated family. Unimarc $d</citation>
            <descriptiveNote>
                <p>Repeatable.</p>
            </descriptiveNote>
        </localTypeDeclaration>

        <maintenanceHistory>
            <maintenanceEvent>
                <eventType>created</eventType>
                <eventDateTime></eventDateTime>
                <agentType></agentType>
                <agent></agent>
